#include<cstdio>
#include<vector>

using namespace std;

int main()
{
	char str[1000];
	
	while(scanf("%s",str)==1)
	{		
		if(str[0]=='e') break;		
				
		if(str[0]=='t') 
		{
			int b,broj;
			sscanf(str+2,"%d",&b);			
			scanf("%d",&broj);
			vector<int> znam;
			int z;	
			
			while(broj)
			{			
				z=broj%b;				
				broj=broj/b;
				if(z<0) 
				{
					z+=abs(b);
					if(b>0) broj--; else broj++;
				}
				
				znam.push_back(z);								
			}
			for(int i=znam.size()-1;i>=0;i--) printf("%d",znam[i]);
			
			printf("\n");
		}						
		else
		{
			char broj[1001];
			int vrati=0,b;
			sscanf(str+4,"%d",&b);			
			scanf("%s", broj);			
			int l=strlen(broj);
			int pot=1;
			for(int i=l-1;i>=0;i--)
			{
				vrati+=(broj[i]-'0')*pot;
				pot*=b;
			}		
			printf("%d\n",vrati);	
		}
	}
		
	return 0;
}