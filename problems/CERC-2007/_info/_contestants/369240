#include <cstdio>
#include <string>

using namespace std;

string k[11] = {"Kamen", "Rock", "Pierre", "Stein", "Ko", "Koe", "Sasso", "Roccia", "Guu", "Kamien", "Piedra"};
string n[10] = {"Nuzky", "Scissors", "Ciseaux", "Schere", "Ollo", "Olloo", "Forbice", "Choki", "Nozyce", "Tijera"};
string p[10] = {"Papir", "Paper", "Feuille", "Papier", "Papir", "Carta", "Rete", "Paa", "Papier", "Papel"};

int num(char* str) {
    string s = string(str);
    for (int i = 0; i < 11; i ++) {
        if (s == k[i]) {
            return 0;
        }
    }
    for (int i = 0; i < 10; i ++) {
        if (s == p[i]) {
            return 1;
        }
    }
    return 2;
}

int main() {
    char l1[100], l2[100];
    char name1[100], name2[100];
    char a[100], b[100];
    int s, t;
    a[0] = '-';
    for (int g = 1; a[0] == '-'; g ++) {
        scanf("%s %s %s %s", l1, name1, l2, name2);
        s = 0;
        t = 0;
        scanf("%s", a);
        while (a[0] != '-' && a[0] != '.') {
            scanf("%s", b);
            switch ((3 + num(a) - num(b)) % 3) {
                case 1:
                    s += 1;
                    break;
                case 2:
                    t += 1;
                    break;
            }
            scanf("%s", a);
        }
        printf("Game #%d:\n", g);
        if (s == 1) {
            printf("%s: %d point\n", name1, s);
        } else {
            printf("%s: %d points\n", name1, s);
        }
        if (t == 1) {
            printf("%s: %d point\n", name2, t);
        } else {
            printf("%s: %d points\n", name2, t);
        }
        if (s == t) {
            printf("TIED GAME\n");
        } else if (s > t) {
            printf("WINNER: %s\n", name1);
        } else {
            printf("WINNER: %s\n", name2);
        }
        printf("\n");
    }
    return 0;
}
