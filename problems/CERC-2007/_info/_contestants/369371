#include <cstdio>
#include <algorithm>
#include <list>
#include <set>
#include <queue>
using namespace std;

#define REP(i,n) for(int i = 0; i < (n); ++i)
#define FOR(i,a,b) for(int i = (a); i <= (b); ++i)
#define FORD(i,a,b) for(int i = (a); i >= (b); --i)
#define PRINTF(args...) printf(args)
// #define PRINTF(args...)

int powa(int baza, int expo)
{
  int res = 1;
  for (int a=0; a<expo; a++)
	res *= baza;
  return res;
}

int naplus(int baza, int dupa)
{
  char t[100];
  baza = -baza;
  sprintf(t, "%d", dupa);
  int wynik = 0, dl = strlen(t);
  for (int i=0; i<dl; i++)
  {
	int expo = dl-i-1;
	wynik += (t[i]-'0') * powa(baza, expo);
  }
  return wynik;
}

int naminus(int baza, int dupa)
{
  char t[50];
  bool znak = false;
  if (dupa<0)
  {
	znak=1;
	dupa *= (-1);
  }
    
  for (int i=0; i<30; i++) t[i] = 0;
  int cnt=0;
  while (dupa)
  {
	t[cnt++] = dupa % baza;
	dupa /= baza;
  }
  if (znak) for (int i=0; i<30; i++) t[i] *= (-1);
  for (int i=0; i<30; i++) if (i%2)
	t[i] *= (-1);
  for (int i=0; i<30; i++)
  {
    if (t[i] >= baza)
	{
	  t[i] -= baza;
	  t[i+1] --;
	}
	else if (t[i]<0)
	{
	  t[i] += baza;
	  t[i+1] ++; 
	}
  }
  char buka[100];
  for (int i=0; i<30; i++)
    buka[i] = t[i]+'0';
  int maxi = 0;
  for (int i=0; i<30; i++)
	if (buka[i] != '-') maxi = i;
  buka[maxi+1] = 0;
  for (int i=0; i<=maxi/2; i++)
	swap(buka[i], buka[maxi-i]);
  int res;
  sscanf(buka, "%d", &res);
  return res;
}


int main() {
  char c;
  while (true)
  {
	c=getchar();
	if (c=='e') break;
	else if (c=='t')
	{
	  int baza, dupa;
	  for (int i=0; i<2; i++) getchar();
	  scanf("%d%d", &baza, &dupa);
	  printf("%d\n", naminus(baza, dupa));
	}
	else
	{
	  int baza, dupa;
	  for (int i=0; i<4; i++) getchar();
	  scanf("%d%d", &baza, &dupa);
	  printf("%d\n", naplus(baza, dupa));
	}
	getchar();
  }
  return 0;
}
