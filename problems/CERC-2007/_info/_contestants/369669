#include <cstdio>
#include <cmath>
#include <vector>
#include <cstring>

using namespace std;


vector<int> p;
int NN;
int R;
int a;
int tmp;
char N[100];
char we[8];


void ToDec()
{
	int n = strlen(N);
	int dec = 0;
	for (int i = n-1; i >= 0; i--)
	{
		dec += ((N[i]-48) * (int)pow((double)R, (int)n-i-1));
		//printf("    N[i]:%d i: %d R: %d wyr: %d dec: %d pow: %d\n",N[i]-48,i,R,((N[i]-'0') * (int)pow((double)R, (int)n-i-1)),dec, (int)pow((double)R, (int)n-i-1));
	}
	printf("%d\n", dec);
}


void ToNeg()
{
/*	int i = 0;
