int main()
{
	scanf("%s", we);
	//printf("pow -10 0 : %d\n\n", (int)pow((double)-10,3));
	while (strcmp(we, "end") != 0)
	{
		if (we[0] == 't')	// do ujemnego
		{
			if (we[4] == 0)
				R = 48 - we[3];
			else
				R = (48 - we[3]) * 10;
			scanf("%d", &NN);
			//printf("R: %d N: %d\n",R, NN);
			ToNeg();
		}
		else	// do dec
		{
			if (we[6] == 0)
				R = 48 - we[5];
			else
				R = (48 - we[5]) * 10;
			scanf("%s", N);
			//printf("R: %d N: %s\n",R, N);
			ToDec();
		}
		scanf("%s",we);
	}
	return 0;
}
