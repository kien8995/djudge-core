	// NN - liczba w dec do konwersji
	// R - podstawa (ujemna!)
	int mxdr = 2000000;
	int mx = mxdr * (-R);
	int tab[1000];
	int cnt = 0;
	int v = NN;
	for (int i = 0; v; i++) {
		tab[cnt++] = (v + mx) % (-R);
