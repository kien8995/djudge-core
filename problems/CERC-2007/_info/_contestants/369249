#include <cstdio>

using namespace std;

int main()
{
	char c, d;
	bool koniec = false;
	int wyj[100];
	int il_cyfr;
	int N, R;
	int m;
	
	while (!koniec)
	{
		switch (getchar())
		{
			case 't':
				getchar();
				getchar();
				scanf("%d%c%d%c", &R, &c, &N, &d);

//				printf("A: %d B: %d\n", R, N);

				il_cyfr = 0;

				while (N != 0)
				{
					m = N % R;

					if (m < 0)
						m += R;

					N -= m;

					wyj[il_cyfr] = m;

					N /= -R;
					++il_cyfr;
				}

				while (il_cyfr > 0)
				{
					printf("%d", wyj[il_cyfr - 1]);
					--il_cyfr;
				}

				putchar('\n');

				break;
			case 'f':
				getchar();
				getchar();
				getchar();
				getchar();
				scanf("%d%c", &R, &c);

				N = 0;

				while ((c = getchar()) != '\n')
				{
					N *= -R;
					N+= c - '0';
				}

				printf("%d\n", N);

				break;
			case 'e':
				koniec = true;
				break;
		}
	}

	return 0;
}

