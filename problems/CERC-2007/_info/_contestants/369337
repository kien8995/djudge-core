#include <stdio.h>
#include <ctype.h>
#include <algorithm>
using namespace std;

#define ABS(a) (((a) < 0) ? -(a) : (a))

int main(void) {
  int b; char c;
  for(;;) {
    while (c=getchar(), isspace(c)) {}
    if (c=='e') break;
    if (c=='f') { getchar(); getchar(); getchar();
      int res=0;
      char cs[100];
      scanf("%d%s",&b, cs);
      for (char *ptr=cs; *ptr; ptr++) res=(res*b)+*ptr-'0';
      printf("%d\n", res);
    } else { getchar();
      int num;
      scanf("%d%d", &b, &num);
      char cs[100]; int len=0;
      while (num) {
	cs[len]=ABS(num) % ABS(b);
	if (num<0 && cs[len]) cs[len]=ABS(b)-cs[len];
	num-=cs[len];
	cs[len++]+='0';
	num/=b;
      }
      cs[len]=0;
      reverse(cs, cs+len);
      puts(cs);
    }
  }
  return 0;
}
