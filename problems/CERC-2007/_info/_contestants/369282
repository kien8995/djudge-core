#include <cstdio>

void to(int n,int b)
{
  if(n==0)
  {
    return;
  }
  int z = ((n%b)+b)%b;

  to(-1*(n-z)/b,b);
  printf("%d",z);
}

int from(int i,char* n,int b)
{
	if(i==-1)
		return 0;
	return (n[i]-'0')+from(i-1,n,b)*b;
}

int main()
{
  for(;;)
  {
    char c = getchar();
    if(c=='e')
      return 0;
    if(c=='t')
    {
      getchar(),getchar();
      int b,n;
      scanf("%d %d ",&b,&n);
      if(n==0)
      {
	      puts("0");
	      continue;
      }
      to(n,b);
      puts("");
    }
    if(c=='f')
    {
      getchar(),getchar(),getchar();
      int b;
      char n[50];
      int i=0;
      scanf("%d %s ",&b,n);
      for(;n[i];i++);

      printf("%d\n",from(i-1,n,b));
    }
  }


}
